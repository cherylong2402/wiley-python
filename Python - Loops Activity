#Acitivity 3
Ex='Hello'
count=0
for i in Ex:
    count+=1
    
print(count)

#Acitivity 4
list = ["a", "bc", "rye", "hello", "c", ""]
new=[]
for i in range(len(list)):
  if len(list[i])>2:
    new+=[list[i]]
print(new)

#Acitivity 5
for i in range(100,1001):
    if i%50==0:
        print(i)
        
while i>100 and i<=1000:
    if i%50==0:
        print(i)

#Acitivity 6
list=range(101)
Sum=sum(list)
print(Sum)

#Acitivity 7
import math
Num=int(input("Enter a number:"))
Fac=math.factorial(Num)
print(Fac)

