#Acitivity 9
list=[]
while True:
    ans=input("Enter a number:")
    list.append(ans)
    if ans=="Quit".lower():
        newlist=list[:-1]
        for i in range(0,len(newlist)):
            newlist[i]=int(newlist[i])
        print(newlist)
        Sum=sum(newlist)
        print("Sum of numbers is");print (Sum)
        break

#Acitivity 10
num=int(input("Enter an integer value:"))
for i in range (0,100):
    print (num**2 * (i))

#Acitivity 11
lower=0
upper=100
for num in range(lower,upper+1):
    if num>1:
        for i in range(2,num):
            if (num%i)==0:
                break
        else:
            print(num)

#Acitivity 12
num1=int(input("Enter first number:"))
num2=int(input("Enter second number:"))
while num2:
    num1, num2 = num2, num1 % num2
print(num1)

#Acitivity 13


#Acitivity 14
x=int(input("Enter first number:"))
y=int(input("Enter second number:"))
def lcm(x, y):
    if x > y:
        greater = x
    else:
        greater = y
while True:
       if ((greater % x == 0) and (greater % y == 0)):
            lcm = greater
            break
            greater += 1
return lcm

